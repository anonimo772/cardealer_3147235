generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
model category {
  id Int 

}
//modelo del pryecto
model brand{

  id_brand Int @id @default(autoincrement())
  name String
}
model car{
  id Int @id @default(autoincrement())
  name String @db.VarChar(15)
 release_date DateTime @db.DateTime()
 price Decimal 
 category_id Int 
 bran_id Int 
 categorY @relations ( resource caterories
  fields:[category_id],
  referecnes:[id_category]
}
